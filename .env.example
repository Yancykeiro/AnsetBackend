# 数据库配置
DATABASE_URL="postgresql://user:password@localhost:5432/anset?schema=public"
# 或者使用 MySQL
# DATABASE_URL="mysql://user:password@localhost:3306/anset"

# 服务器配置
PORT=3000
HOST=0.0.0.0

# 阿里云通义千问配置
TONGYI_API_KEY=your_tongyi_api_key_here
TONGYI_ENDPOINT=https://dashscope.aliyuncs.com/api/v1

# 文件上传配置
UPLOAD_DIR=./uploads
MAX_FILE_SIZE=10485760

# JWT密钥（可选，用于用户认证）
JWT_SECRET=your_secret_key_here

SSL_KEY_PATH=/etc/ssl/anset/anset.top.key
SSL_CERT_PATH=/etc/ssl/anset/anset.top.pem
